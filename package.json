{
  "name": "logseq-jot-theme",
  "version": "3.7.3",
  "logseq-version": "v0.0.1",
  "author": "shaunrfox",
  "description": "",
  "main": "source/index.sass",
  "scripts": {
    "watch": "sass --watch source/index.scss ~/Library/'Mobile Documents'/iCloud~com~logseq~logseq/Documents/logseq/custom.css --no-source-map && npx uglifycss",
    "build": "sass source/index.scss custom.css --no-source-map && npx uglifycss custom.css --output ~/Library/'Mobile Documents'/iCloud~com~logseq~logseq/Documents/logseq/custom.css"
  },
  "devDependencies": {
    "sass": "^1.34.1",
    "uglifycss": "^0.0.29"
  },
  "logseq": {
    "themes": [
      {
        "name": "Jot Theme",
        "url": "./custom.css",
        "description": ""
      }
    ],
    "id": "shaunrfox_jot_theme",
    "icon": "./icon.png"
  }
}